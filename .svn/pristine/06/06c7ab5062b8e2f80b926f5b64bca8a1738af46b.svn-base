package com.example.bean;

import java.util.Date;

/**
 * 事件追踪实体类
 * @author binLee
 *
 */
public class EventTrackEntity {
	
	private int eventId;           // 事件ID值
	private int dataType;          // 事件追踪接口类型  1~ 话题演化  2~热词推荐  3~今日话题  4~情感分析  5~渠道类型  6~报道类型
	private int deleteFlag;        // 删除标识位
	private Date lastModifiedTime; // 更新时间
	private String content;        // 事件内容，对应于：话题演化~title  热词推荐~keyword  今日话题~title
	private int count;             // 事件每个话题或热词的关注量，对应于：话题演化~count  热词推荐~frequency  今日话题~count
	private Date clusterTime;      // 数据在内容大数据平台的实际采集时间
	private String emotionAnalysis;// 情感分析
	private String channelType;    // 渠道类型
	private String reportTrend;    // 报道趋势
	private String reportsRank;    // 媒体报道量排行
	private Date apiLastModifiedTime; // 内容大数据接口数据最后的更新时间
	
	public int getEventId() {
		return eventId;
	}
	public void setEventId(int eventId) {
		this.eventId = eventId;
	}
	public int getDataType() {
		return dataType;
	}
	public void setDataType(int dataType) {
		this.dataType = dataType;
	}
	public int getDeleteFlag() {
		return deleteFlag;
	}
	public void setDeleteFlag(int deleteFlag) {
		this.deleteFlag = deleteFlag;
	}
	public Date getLastModifiedTime() {
		return lastModifiedTime;
	}
	public void setLastModifiedTime(Date lastModifiedTime) {
		this.lastModifiedTime = lastModifiedTime;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public Date getClusterTime() {
		return clusterTime;
	}
	public void setClusterTime(Date clusterTime) {
		this.clusterTime = clusterTime;
	}
	public String getEmotionAnalysis() {
		return emotionAnalysis;
	}
	public void setEmotionAnalysis(String emotionAnalysis) {
		this.emotionAnalysis = emotionAnalysis;
	}
	public String getChannelType() {
		return channelType;
	}
	public void setChannelType(String channelType) {
		this.channelType = channelType;
	}
	public String getReportTrend() {
		return reportTrend;
	}
	public void setReportTrend(String reportTrend) {
		this.reportTrend = reportTrend;
	}
	public String getReportsRank() {
		return reportsRank;
	}
	public void setReportsRank(String reportsRank) {
		this.reportsRank = reportsRank;
	}
	public Date getApiLastModifiedTime() {
		return apiLastModifiedTime;
	}
	public void setApiLastModifiedTime(Date apiLastModifiedTime) {
		this.apiLastModifiedTime = apiLastModifiedTime;
	}
	
}
